generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Plant {
  id        String @id @default(cuid())
  positionX Int
  positionY Int
  plot      Plot   @relation(fields: [plotId], references: [id])
  plotId    Int
}

model Plot {
  id    Int     @id @default(autoincrement())
  name  String
  Plant Plant[]
}
